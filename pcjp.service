[Unit]
Description=PhenoControl-JP
After=network.target

[Service]
Type=simple
ExecStart=/home/ec2-user/PhenoControl-JP/run.sh 8080
ExecStop=/home/ec2-user/PhenoControl-JP/stop.sh 8080
Restart=on-failure
RestartSec=5
User=ec2-user
WorkingDirectory=/home/ec2-user/PhenoControl-JP

[Install]
WantedBy=multi-user.target
